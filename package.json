{
  "name": "mdx-go",
  "version": "1.2.0-2",
  "description": "Lightning-fast MDX-based dev server",
  "main": "dist/index.js",
  "bin": {
    "mdx-go": "./cli.js"
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "npm urn clean && npm run babel && npm run emotion && npm run styled-components",
    "clean": "rm -rf dist",
    "babel": "babel src -d dist --ignore styled",
    "watch": "babel src -d dist --watch",
    "emotion": "NODE_ENV=emotion babel src/styled -d dist/emotion",
    "styled-components": "NODE_ENV=styled-components babel src/styled -d dist/styled-components",
    "start": "./cli.js docs",
    "build": "./cli.js build docs -d site",
    "help": "./cli.js",
    "test": "nyc ava",
    "logo": "repng docs/logo.js -w 256 -h 256 -d docs -f logo.png",
    "card": "repng docs/card.js -w 1024 -h 512 -d docs -f card.png"
  },
  "keywords": [
    "mdx",
    "react",
    "dev-server",
    "development",
    "docs",
    "documentation",
    "static-site"
  ],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "@mdx-js/loader": "^0.15.2",
    "@mdx-js/mdx": "^0.15.2",
    "@mdx-js/tag": "^0.15.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "connect": "^3.6.6",
    "connect-history-api-fallback": "^1.5.0",
    "emotion": "^9.2.8",
    "emotion-theming": "^9.2.6",
    "find-up": "^3.0.0",
    "is-absolute-url": "^2.1.0",
    "lodash.get": "^4.4.2",
    "lodash.sortby": "^4.7.0",
    "meow": "^5.0.0",
    "pkg-conf": "^2.1.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.1",
    "react-emotion": "^9.2.8",
    "react-live": "^1.11.0",
    "react-router-dom": "^4.3.1",
    "read-pkg-up": "^4.0.0",
    "remark-autolink-headings": "^5.0.0",
    "remark-emoji": "^2.0.1",
    "remark-images": "^0.8.1",
    "remark-slug": "^5.1.0",
    "remark-unwrap-images": "0.0.2-0",
    "resolve-cwd": "^2.0.0",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.2",
    "styled-system": "^3.1.3",
    "update-notifier": "^2.5.0",
    "webpack": "^4.19.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.24.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "nyc": "^12.0.2",
    "supertest": "^3.1.0"
  }
}
